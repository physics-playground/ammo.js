{
    "name": "ammo.js",
    "version": "0.0.2",
    "description": "Direct port of the Bullet physics engine to JavaScript using Emscripten",
    "main": "bin/ammo.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/kripken/ammo.js.git"
    },
    "author": "Alon Zakai <alonzakai@gmail.com>",
    "bugs": {
        "url": "https://github.com/kripken/ammo.js/issues"
    },
    "homepage": "https://github.com/kripken/ammo.js",
    "devDependencies": {
        "ava": "^3.11.0",
        "cross-env": "^7.0.3",
        "shelljs": "^0.8.5",
        "http-server": "^14.1.0"
    },
    "ava": {
        "files": [
            "./tests/**",
            "!tests/**/{fixtures,helpers}/**"
        ],
        "ignoredByWatcher": [
            "{build}/**"
        ],
        "environmentVariables": {}
    },
    "scripts": {
        "test": "npm run test-js && npm run test-wasm",
        "test-js": "cross-env AMMO_PATH=bin/ammo.js ava",
        "test-wasm": "cross-env AMMO_PATH=bin/ammo.wasm.js ava"
    },
    "dependencies": {}
}
